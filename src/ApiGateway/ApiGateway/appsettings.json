{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "authRoute": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/auth"
          }
        ]
      },
      "groupsRoute": {
        "ClusterId": "groupsCluster",
        "Match": {
          "Path": "/groups/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/groups"
          }
        ]
      },
      "vehiclesRoute": {
        "ClusterId": "vehiclesCluster",
        "Match": {
          "Path": "/vehicles/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/vehicles"
          }
        ]
      },
      "bookingsRoute": {
        "ClusterId": "bookingsCluster",
        "Match": {
          "Path": "/bookings/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/bookings"
          }
        ]
      },
      "financeRoute": {
        "ClusterId": "financeCluster",
        "Match": {
          "Path": "/finance/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/finance"
          }
        ]
      }
    },
    "Clusters": {
      "authCluster": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:5228"
          }
        }
      },
      "groupsCluster": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:5196"
          }
        }
      },
      "vehiclesCluster": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:5010"
          }
        }
      },
      "bookingsCluster": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:5248"
          }
        }
      },
      "financeCluster": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:5020"
          }
        }
      }
    }
  }
}
